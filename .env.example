POSTGRES_USER="bs_value"
POSTGRES_HOST="bs_value"
POSTGRES_PASSWORD="bs_value"
POSTGRES_DATABASE="bs_value"

# Clerk
NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY=bs_value
